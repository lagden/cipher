{
  "name": "@xet/cipher",
  "version": "3.0.0",
  "description": "Encrypt and decrypt",
  "repository": "lagden/cipher",
  "main": "index.js",
  "files": [
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "report": "nyc report --reporter=text-lcov | coveralls",
    "pretest": "xo",
    "test": "ava"
  },
  "keywords": [
    "crypto",
    "cipher",
    "lib"
  ],
  "author": "Thiago Lagden",
  "license": "MIT",
  "engines": {
    "node": ">=8.9.1"
  },
  "devDependencies": {
    "ava": "0.25.0",
    "xo": "0.23.0"
  },
  "xo": {
    "envs": [
      "node"
    ],
    "semicolon": false,
    "rules": {
      "camelcase": [
        "error",
        {
          "properties": "never"
        }
      ],
      "capitalized-comments": 0
    }
  }
}
